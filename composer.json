{
    "name": "rse-sa/dynaflow",
    "description": "Dynamic Workflows For Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhannad A",
            "email": "malhussain@rse.sa"
        }
    ],
    "require": {
        "illuminate/support": ">=10.0",
        "spatie/laravel-translatable": "^6.0"
    },
    "suggest": {},
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "orchestra/testbench": ">=8.0",
        "mockery/mockery": "^1.2",
        "laravel/framework": ">=10.0"
    },
    "autoload": {
        "files": [
            "src/Helpers.php"
        ],
        "psr-4": {
            "RSE\\DynaFlow\\": "src/",
            "RSE\\DynaFlow\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RSE\\DynaFlow\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RSE\\DynaFlow\\DynaflowServiceProvider"
            ],
            "aliases": {
                "Workflow": "RSE\\DynaFlow\\Facades\\Dynaflow"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
